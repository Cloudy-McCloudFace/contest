<!DOCTYPE html>
<html lang="en">
<head>
  <meta charset="UTF-8" />
  <title>OVHcloud Contest</title>
</head>
<body>
    <header>
        <ul>
          <li><a href="/">Front</a></li>
          @loggedIn
            <li><a href="/home">Home/Dashboard</a></li> 
            <li><a href="/submit-startup">Submit Start up</a></li>  
          @endloggedIn
          <li><a href="/login">Login</a></li>  
          <li><a href="/register">Register</a></li>  
        </ul>
      </header>
  <section>
    <h1>Submit Startup</h1>
    <form action="{{ route('submit-startup') }}" method="POST">
       {{ csrfField() }}
      <label for="name">name</label>
      <input type="text" name="name" id="name">

      <label for="description">description</label>
      <input type="textarea" name="description" id="description">

      <label for="url">url</label>
      <input type="text" name="url" id="url">

      <label for="logo_url">logo_url</label>
      <input type="text" name="logo_url" id="logo_url">
      
      <input type="submit" value="submit">
    </form>
  </section>
</body>
</html>
